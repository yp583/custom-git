{
	"name": "term-ui",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "node build.js",
		"dev": "tsc --watch",
		"test": "prettier --check . && xo && ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@inkjs/ui": "^2.0.0",
		"drawille": "^2.0.2",
		"execa": "^9.6.0",
		"ink": "^5.2.1",
		"meow": "^13.2.0",
		"react": "^18.3.1",
		"react-devtools-core": "^4.28.5",
		"simple-git": "^3.30.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/react": "^18.3.18",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^6.2.0",
		"chalk": "^5.4.1",
		"esbuild": "^0.27.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.37.2",
		"eslint-plugin-react-hooks": "^5.2.0",
		"ink-testing-library": "^4.0.0",
		"prettier": "^3.5.3",
		"ts-node": "^10.9.2",
		"typescript": "^5.7.3",
		"xo": "^0.60.0"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
